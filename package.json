        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-koa-bodyparser",
    "keywords": [
        "documentation",
        "koa-bodyparser"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_koa_bodyparser.js",
    "name": "npmdoc-koa-bodyparser",
    "nameAlias": "npmdoc_koa_bodyparser",
